    {
      "name": "JS Example",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/index.js",
      "args": ["${workspaceFolder}/examples/javascript"],
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Rust Example",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/index.js",
      "args": ["${workspaceFolder}/examples/rust"],
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
